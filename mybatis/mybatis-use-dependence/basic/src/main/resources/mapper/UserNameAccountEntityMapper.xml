<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.s2u2m.lab.mybatis.dependence.basic.mapper.UserNameAccountMapper">

    <sql id="all_fields">
        userId, userName, password, createTime
    </sql>

    <insert id="insert" parameterType="com.s2u2m.lab.mybatis.dependence.basic.mapper.UserNameAccountEntity">
        insert into username_account (
        <include refid="all_fields"/>
        ) values (
        #{userId}, #{userName}, #{password}, #{createTime}
        )
    </insert>

</mapper>